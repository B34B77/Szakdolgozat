!function(){function t(t){for(var e=t.srcElement||t.target;e&&("undefined"==typeof e.tagName||"a"!=e.tagName.toLowerCase()||!e.href);)e=e.parentNode;if(e&&e.href){var n=e.href;if(-1==n.indexOf(location.host)&&!n.match(/^javascript\:/i)){var a=function(t,e){e?window.open(t,e):window.location.href=t},o=e.target&&!e.target.match(/^_(self|parent|top)$/i)?e.target:!1;ga("send","event","Outbound Link",n,document.location.pathname+document.location.search,{hitCallback:a(n,o)}),t.preventDefault?t.preventDefault():t.returnValue=!1}}}var e=window;e.addEventListener?e.addEventListener("load",function(){document.body.addEventListener("click",t,!1)},!1):e.attachEvent&&e.attachEvent("onload",function(){document.body.attachEvent("onclick",t)})}();
